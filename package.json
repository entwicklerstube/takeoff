{
  "name": "takeoff",
  "version": "5.0.0",
  "description": "Scaffolding like it should be. Easy, fast and clean.",
  "main": "takeoff.js",
  "scripts": {
    "start": "babel-node index",
    "test": "mocha ./lib/**/test.js ./utils/**/test.js -r ./config/mocha",
    "test:coverage": "nyc --reporter=html --reporter=text mocha ./lib/**/test.js ./utils/**/test.js -r ./config/mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf dist",
    "build": "rollup --config config/rollup.js",
    "takeoff": "takeoff"
  },
  "files": [
    "takeoff.js",
    "utils.js"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entwicklerstube/takeoff.git"
  },
  "bin": {
    "takeoff": "index.js"
  },
  "keywords": [
    "scaffolding",
    "takeoff"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/entwicklerstube/takeoff/issues"
  },
  "homepage": "https://github.com/entwicklerstube/takeoff#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "change-case": "^3.0.2",
    "commander": "^2.19.0",
    "coveralls": "^3.0.2",
    "debug": "^4.1.0",
    "eslint": "^5.7.0",
    "inquirer": "^6.2.0",
    "inquirer-test": "^2.0.1",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "mock-fs": "^4.7.0",
    "mock-prompt": "^0.1.3",
    "mock-require": "^3.0.2",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "takeoff-cli": "^4.0.0",
    "unexpected": "^10.39.1"
  }
}
